{
  "name": "emoji-lookup",
  "version": "0.0.1",
  "description": "An extension for finding and inserting emoji into text inputs",
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "alarms",
    "sessions",
    "history",
    "bookmarks",
    "storage",
    "contextMenus"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "manifest_version": 2,
  "commands": {
    "toggleExtension": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "Alt+Space"
      },
      "description": "Toggle Saka"
    }
  },
  "web_accessible_resources": ["popup.html"],
  "browser_action": {
    "default_icon": {
      "48": "logo.png"
    }
  }
}
