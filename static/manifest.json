{
  "name": "emoji-lookup",
  "version": "0.0.1",
  "description": "An extension for finding and inserting emoji into text inputs",
  "permissions": ["clipboard"],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "mac": "Alt+Space"
      },
      "description": "Toggle Saka"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "applications": {
    "gecko": {
      "id": "{4c7335d3-74e0-4fe0-81f5-5f1f35a47691}"
    }
  }
}
